{
  "name": "luigi-core-private",
  "private": true,
  "description": "luigi-core development version, only the public folder is being published to npm as a package",
  "devDependencies": {
    "@babel/node": "^7.18.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@sap-theming/theming-base-content": "11.1.35",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "bundlesize": "^0.18.1",
    "chai": "^4.3.6",
    "esm": "^3.2.25",
    "fundamental-styles": "0.23.0",
    "jsdom": "^20.0.0",
    "jsdom-global": "^3.0.2",
    "live-server": "^1.2.2",
    "lodash": "^4.17.21",
    "lodash.template": "^4.5.0",
    "mocha": "^10.0.0",
    "mock-local-storage": "^1.1.23",
    "nyc": "^15.1.0",
    "oidc-client": "^1.11.5",
    "sass": "^1.54.0",
    "sinon": "^14.0.0",
    "svelte": "^3.49.0",
    "svelte-preprocess": "^4.10.7",
    "svelte-transitions": "^1.2.0",
    "vite": "^3.0.3"
  },
  "scripts": {
    "test": "jasmine",
    "test_old": "babel-node ./node_modules/nyc/bin/nyc.js mocha -- --require esm --recursive ./test",
    "test:debug": "npm run test -- --inspect-brk ./test/services/routing.spec.js --watch",
    "test:watch": "npm run test -- --watch",
    "bundlesize": "npm run build && bundlesize",
    "bundlesizeOnly": "bundlesize",
    "helper:webpack-analyze": "ANALYZE=true npm run bundle-evergreen",
    "build": "vite build",
    "watch": "vite build --watch",
    "simpledev": "node ./dev-tools/simpledev-vite.mjs"
  },
  "bundlesize": [
    {
      "path": "./dist/luigi.js",
      "maxSize": "650 kB",
      "compression": "none"
    },
    {
      "path": "./dist/luigi.css",
      "maxSize": "650 kB",
      "compression": "none"
    }
  ],
  "nyc": {
    "check-coverage": true,
    "lines": 55,
    "statements": 55,
    "functions": 54,
    "branches": 55,
    "watermarks": {
      "lines": [
        55,
        70
      ],
      "functions": [
        55,
        60
      ],
      "branches": [
        55,
        65
      ],
      "statements": [
        55,
        70
      ]
    },
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "require": [
      "@babel/register",
      "jsdom-global/register"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "cache": true,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/SAP/luigi.git"
  }
}
